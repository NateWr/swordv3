msgid "plugins.generic.swordv3.name"
msgstr "SWORDv3 Deposits"

msgid "plugins.generic.swordv3.description"
msgstr "Send published metadata and PDF galleys to a repository that accepts deposits according to the SWORDv3 protocol."

msgid "plugins.generic.swordv3.serviceRequired"
msgstr "You must configure a deposit service."

msgid "plugins.generic.swordv3.service.name"
msgstr "Service Name"

msgid "plugins.generic.swordv3.service.url"
msgstr "Service URL"

msgid "plugins.generic.swordv3.service.authMode"
msgstr "Authentication Method"

msgid "plugins.generic.swordv3.service.authMode.unsupported"
msgstr "The {$authMode} authentication method is not supported by this service. Supported modes: {$supportedModes}"

msgid "plugins.generic.swordv3.service.authMode.userPassFailed"
msgstr "The username or password are incorrect."

msgid "plugins.generic.swordv3.service.authMode.apiKeyFailed"
msgstr "The API Key is incorrect."

msgid "plugins.generic.swordv3.service.authMode.basic"
msgstr "Username and Password"

msgid "plugins.generic.swordv3.service.authMode.apiKey"
msgstr "API Key"

msgid "plugins.generic.swordv3.service.username"
msgstr "Username"

msgid "plugins.generic.swordv3.service.password"
msgstr "Password"

msgid "plugins.generic.swordv3.service.apiKey"
msgstr "API Key"

msgid "plugins.generic.swordv3.service.apiKey.description"
msgstr "The unique key provided to you by the service."

msgid "plugins.generic.swordv3.service.setupFailed"
msgstr "Can't connect to the service. Please check the details and try again. If the problem persists, please contact the service provider for help."

msgid "plugins.generic.swordv3.service.depositsNotAccepted"
msgstr "Connected successfully to the service, but it does not accept deposits. Please contact the service provider for more information."

msgid "plugins.generic.swordv3.service.digestNotAccepted"
msgstr "No compatible digest format could be found to validate file integrity with this deposit service."

msgid "plugins.generic.swordv3.status.ready"
msgstr "Ready to Deposit"

msgid "plugins.generic.swordv3.status.ready.description"
msgstr "Published items that have not yet been deposited."

msgid "plugins.generic.swordv3.status.ready.withCount"
msgstr "There are {$count} publications ready to be deposited."

msgid "plugins.generic.swordv3.status.deposited"
msgstr "Deposited"

msgid "plugins.generic.swordv3.status.deposited.description"
msgstr "Deposits ingested, accepted or queued for processing by the service."

msgid "plugins.generic.swordv3.status.deposited.withCount"
msgstr "There are {$count} publications that have been successfully deposited."

msgid "plugins.generic.swordv3.status.rejected"
msgstr "Rejected"

msgid "plugins.generic.swordv3.status.rejected.description"
msgstr "Deposits rejected by the service."

msgid "plugins.generic.swordv3.status.rejected.withCount"
msgstr "There are {$count} publications that have been rejected by the deposit service."

msgid "plugins.generic.swordv3.status.deleted"
msgstr "Deleted"

msgid "plugins.generic.swordv3.status.deleted.description"
msgstr "Deposits deleted by the service."

msgid "plugins.generic.swordv3.status.deleted.withCount"
msgstr "There are {$count} publications that have been deleted by the deposit service."

msgid "plugins.generic.swordv3.status.unknown"
msgstr "Unknown"

msgid "plugins.generic.swordv3.status.unknown.description"
msgstr "Deposits in an unknown state."

msgid "plugins.generic.swordv3.status.unknown.withCount"
msgstr "There are {$count} publications that have an unrecognized status. If a publication has an unrecognized status, it may indicate a problem with the deposit."

msgid "plugins.generic.swordv3.disabled"
msgstr "Deposits to this service were disabled automatically when an error was encountered."

msgid "plugins.generic.swordv3.disabled.help"
msgstr "If you are unable to resolve the issue yourself, please contact the depositing service or your system administrator for help. Once resolved, you can resume automated deposits by re-saving the form below."

msgid "plugins.generic.swordv3.authError.missingCredentials"
msgstr "No authenticiation credentials sent with deposit request. Please provide the following error message to your system administrator: {$error}"

msgid "plugins.generic.swordv3.notification.depositError.subject"
msgstr "Error depositing publications from {$context} in {$service}"

msgid "plugins.generic.swordv3.notification.authError.intro"
msgstr "An authentication error occurred while trying to deposit metadata and files from <a href=\"{$contextUrl}\">{$context}</a> in the deposit service <a href=\"{$serviceUrl}\">{$service}</a>."

msgid "plugins.generic.swordv3.notification.serviceStatus.intro"
msgstr "A problem was encountered while trying to deposit metadata and files from <a href=\"{$contextUrl}\">{$context}</a> in the deposit service <a href=\"{$serviceUrl}\">{$service}</a>."

msgid "plugins.generic.swordv3.notification.depositsStopped"
msgstr "All deposits have been stopped and will not resume until the problem has been resolved. You may need to adjust the <a href=\"{$url}\">service settings</a>."

msgid "plugins.generic.swordv3.notification.recipientsStatement"
msgstr "You are receiving this email because you are a Journal Manager, Site Administrator, or Technical Support Contact for <a href=\"{$url}\">{$context}</a>."

msgid "plugins.generic.swordv3.notification.automated"
msgstr "This is an automated email sent by the {$plugin} plugin."

msgid "plugins.generic.swordv3.notification.recipientsNotFound"
msgstr "Unable to send a notification email about a failed deposit. No valid recipients were found among journal managers, site administrator, or the technical support contact."

msgid "plugins.generic.swordv3.task.checkInProgressDeposits"
msgstr "Update status of in progress swordv3 deposits"

msgid "plugins.generic.swordv3.action.deposit"
msgstr "Deposit"

msgid "plugins.generic.swordv3.action.redeposit"
msgstr "Re-deposit"

msgid "plugins.generic.swordv3.action.depositsQueued"
msgstr "Queued {$count} publications for deposit at {$service}. It may take several minutes or hours to complete the deposits."

msgid "plugins.generic.swordv3.action.exportToCsv"
msgstr "Export to CSV"

msgid "plugins.generic.swordv3.action.redeposit"
msgstr "Re-deposit"

msgid "plugins.generic.swordv3.action.redepositAll"
msgstr "Re-deposit All Publications"

msgid "plugins.generic.swordv3.action.redepositAll.description"
msgstr "Force all publications to be deposited again. This will replace all existing deposit data and should be avoided if possible. Only use this feature if your existing deposits are faulty or incomplete, or when the service provider has asked you to redeposit all publications due to a change in their service."

msgid "plugins.generic.swordv3.action.redepositAll.button"
msgstr "Re-deposit All"

msgid "plugins.generic.swordv3.action.redepositRejected"
msgstr "Re-deposit Rejected"

msgid "plugins.generic.swordv3.action.redepositDeleted"
msgstr "Re-deposit Deleted"

msgid "plugins.generic.swordv3.action.redepositUnknown"
msgstr "Re-deposit Unknown"

msgid "plugins.generic.swordv3.action.redeposit.confirm"
msgstr "Are you sure you want to re-deposit <strong>{$count} publications</strong> in {$service}?"

msgid "plugins.generic.swordv3.action.tryAgain"
msgstr "Try Again"

msgid "plugins.generic.swordv3.processingQueue"
msgstr "Depositing {$count}"

msgid "plugins.generic.swordv3.titleWithVersion"
msgstr "({$version}) {$title}"

msgid "plugins.generic.swordv3.dateDeposited"
msgstr "Date Deposited"

