msgid "plugins.generic.swordv3.name"
msgstr "SWORDv3 Deposits"

msgid "plugins.generic.swordv3.description"
msgstr "Send published metadata and PDF galleys to a repository that accepts deposits according to the SWORDv3 protocol."

msgid "plugins.generic.swordv3.service.name"
msgstr "Service Name"

msgid "plugins.generic.swordv3.service.url"
msgstr "Service URL"

msgid "plugins.generic.swordv3.service.authMode"
msgstr "Authentication Method"

msgid "plugins.generic.swordv3.service.authMode.unsupported"
msgstr "The {$authMode} authentication method is not supported by this service. Supported modes: {$supportedModes}"

msgid "plugins.generic.swordv3.service.authMode.userPassFailed"
msgstr "The username or password are incorrect."

msgid "plugins.generic.swordv3.service.authMode.apiKeyFailed"
msgstr "The API Key is incorrect."

msgid "plugins.generic.swordv3.service.authMode.basic"
msgstr "Username and Password"

